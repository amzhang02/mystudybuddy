{
    "name": "My Study Buddy Settings",
    "version": "1.0.0",
    "description": "Settings page for reminders extension",
    "permissions": ["tabs", "http://*/*", "https://*/*", "activeTab"],
    "manifest_version": 3,
    "background": {
        "service_worker": "ext_tab.js"
    },
    "action": {
        "default_title": "Settings tab for My Study Buddy",
        "default_popup": "ext_tab.html"
    }
}